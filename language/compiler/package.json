{
  "name": "@sigil-lang/compiler",
  "version": "0.1.0",
  "description": "Sigil programming language compiler",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "pnpm run test:canonical && pnpm run test:unit",
    "test:unit": "pnpm exec node --import tsx --test test/**/*.test.ts",
    "test:canonical": "cd .. && ./test-canonical.sh",
    "pretest": "npm run build",
    "compile": "node dist/cli.js"
  },
  "keywords": [
    "sigil",
    "compiler",
    "programming-language",
    "ai-first",
    "functional"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {}
}
