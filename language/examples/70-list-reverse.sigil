⟦ Test: List Reversal
   Expected output: 3
   Reverse list and get first element
⟧

λ reverse_helper(xs: [ℤ], acc: [ℤ]) → [ℤ] ≡xs {
  [] → acc |
  [x, .rest] → reverse_helper(rest, [x, ..acc])
}

λ reverse(xs: [ℤ]) → [ℤ] = reverse_helper(xs, [])

λ head(xs: [ℤ]) → ℤ ≡xs {
  [x, ..] → x |
  [] → 0
}

λ main() → ℤ = head(reverse([1, 2, 3]))
