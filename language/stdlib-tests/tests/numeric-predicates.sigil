i stdlib⋅numeric_predicates

test "numeric_predicates.in_range is inclusive" {
  stdlib⋅numeric_predicates.in_range(3,1,3)=⊤∧stdlib⋅numeric_predicates.in_range(4,1,3)=⊥
}

test "numeric_predicates.is_even and is_odd basics" {
  stdlib⋅numeric_predicates.is_even(4)=⊤∧stdlib⋅numeric_predicates.is_odd(5)=⊤
}

test "numeric_predicates.is_positive and is_negative basics" {
  stdlib⋅numeric_predicates.is_positive(5)=⊤∧stdlib⋅numeric_predicates.is_negative(-1)=⊤
}
