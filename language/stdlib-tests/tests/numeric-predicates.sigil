i stdlib⋅numeric

test "numeric.in_range is inclusive" {
  stdlib⋅numeric.in_range(3,1,3)=⊤∧stdlib⋅numeric.in_range(4,1,3)=⊥
}

test "numeric.is_even and is_odd basics" {
  stdlib⋅numeric.is_even(4)=⊤∧stdlib⋅numeric.is_odd(5)=⊤
}

test "numeric.is_positive and is_negative basics" {
  stdlib⋅numeric.is_positive(5)=⊤∧stdlib⋅numeric.is_negative(-1)=⊤
}
